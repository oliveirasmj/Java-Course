public class Teste {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i=3;
		System.out.println(i++);
		//System.out.println(++i);
		System.out.println(i);
	}

}

//	num=num+1
//	num+=1

//	num++  --> valor antigo será usado para o cálculo
//	++num  --> valor antigo não será usado para o cálculo
