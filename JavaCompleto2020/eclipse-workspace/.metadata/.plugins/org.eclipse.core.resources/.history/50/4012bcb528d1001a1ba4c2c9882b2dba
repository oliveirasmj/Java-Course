package secao6;

import java.util.Scanner;

public class Main3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		int somaAlcool=0, somaGasolina=0, somaDiesel=0;
		
		while (x==1 || x==2 || x==3) {
			if (x<1 || x>3) {
				if(x!=4)
				x = sc.nextInt();
				else {
					break;
				}
			}
			else if(x==1) {
				somaAlcool+=1;
			} else if(x==2){
				somaGasolina+=1;
			} else if(x==3){
				somaDiesel+=1;
			}
			
			//ler novamente variaveis
			x = sc.nextInt();
		}
		sc.close();
	}

}
